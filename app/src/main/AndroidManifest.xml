            android:screenOrientation="portrait" />
        <activity
            android:name=".Modify_Routine.AddClassTuesday"
            android:parentActivityName=".Activity.RoutineActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name=".Modify_Routine.AddClassWednesday"
            android:parentActivityName=".Activity.RoutineActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name=".Modify_Routine.AddClassThursday"
